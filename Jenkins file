node {
    def app
    
     stage('Initialize'){
        def dockerHome = tool 'myDocker'
        env.PATH = "${dockerHome}/bin:${env.PATH}
     }
     
     stage('Clone repository') {
         checkout scm
     }
     
     stage('Build image') {
         app = docker.build("srinathramanathan/docker")
     }
     
     stage('Text image') {
         app.inside {
             sh 'echo "Tests passed"'
         }
